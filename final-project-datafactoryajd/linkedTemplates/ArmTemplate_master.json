{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "final-project-datafactoryajd"
		},
		"ls_adlsnycpayrollalexjdv1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adlsnycpayrollalexjdv1'"
		},
		"ls_payrollsqldb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_payrollsqldb'"
		},
		"ls_synapse_sql_pool_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_synapse_sql_pool'"
		},
		"ls_adlsnycpayrollalexjdv1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsnycpayrollalexjdv1.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "final-project-datafactoryajd_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adlsnycpayrollalexjdv1_accountKey": {
						"value": "[parameters('ls_adlsnycpayrollalexjdv1_accountKey')]"
					},
					"ls_payrollsqldb_connectionString": {
						"value": "[parameters('ls_payrollsqldb_connectionString')]"
					},
					"ls_synapse_sql_pool_connectionString": {
						"value": "[parameters('ls_synapse_sql_pool_connectionString')]"
					},
					"ls_adlsnycpayrollalexjdv1_properties_typeProperties_url": {
						"value": "[parameters('ls_adlsnycpayrollalexjdv1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "final-project-datafactoryajd_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'final-project-datafactoryajd_ArmTemplate_0')]"
			]
		}
	]
}